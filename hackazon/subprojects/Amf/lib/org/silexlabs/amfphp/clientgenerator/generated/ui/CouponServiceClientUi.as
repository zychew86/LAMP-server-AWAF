package org.silexlabs.amfphp.clientgenerator.generated.ui{	import fla.Label;	import fla.Button;	import fla.TextInput;	import fla.TextResultDisplay;		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.net.NetConnection;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFieldType;		import org.silexlabs.amfphp.clientgenerator.generated.service.CouponServiceClient;	import org.silexlabs.amfphp.clientgenerator.NetConnectionSingleton;	import org.silexlabs.amfphp.clientgenerator.ObjectUtil;	import org.silexlabs.amfphp.clientgenerator.ui.elements.IDataIoGui;	import org.silexlabs.amfphp.clientgenerator.ui.elements.ILabel;	import org.silexlabs.amfphp.clientgenerator.generated.service.CouponServiceClient;
		public class CouponServiceClientUi extends MovieClip	{		/**		 * layout constants		 * */		private static const PADDING:int = 20;		private static const METHOD_UI_HEIGHT:int = 40;		private static const METHOD_INPUT_WIDTH:int = 120;		private static const CALL_BUTTON_X:int = 400;				private var _service:CouponServiceClient;						//useCoupon inputs 		public var useCoupon_couponCode_Input:IDataIoGui; 		public var useCouponLabel:ILabel;		public var useCoupon_callButton:ILabel;				//unsetCoupon inputs 		public var unsetCouponLabel:ILabel;		public var unsetCoupon_callButton:ILabel;				public var resultDisplay:IDataIoGui;		public function CouponServiceClientUi()		{			super();			var xLayoutMultiplier:int = 0;			var yLayoutMultiplier:int = 0;			var maxNumMethodInputs:int = 0;						_service = new CouponServiceClient(NetConnectionSingleton.getNetConnection());			graphics.beginFill(0, 0.3);						//useCoupon ui			xLayoutMultiplier = 0;			if(!useCouponLabel){				useCouponLabel = new Label();				useCouponLabel.label = 'useCoupon method';				useCouponLabel.displayObject.x = PADDING;				useCouponLabel.displayObject.y = PADDING + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(useCouponLabel.displayObject);							}									if(!useCoupon_couponCode_Input){				useCoupon_couponCode_Input = new TextInput();				useCoupon_couponCode_Input.displayObject.x = PADDING + METHOD_INPUT_WIDTH * xLayoutMultiplier;				useCoupon_couponCode_Input.displayObject.y = PADDING + useCouponLabel.displayObject.height  + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(useCoupon_couponCode_Input.displayObject);							}			xLayoutMultiplier++;									if(!useCoupon_callButton){				useCoupon_callButton = new Button();				useCoupon_callButton.label = "call";				useCoupon_callButton.displayObject.x = PADDING + METHOD_INPUT_WIDTH * xLayoutMultiplier;				useCoupon_callButton.displayObject.y = PADDING + useCouponLabel.displayObject.height  + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(useCoupon_callButton.displayObject);			}			useCoupon_callButton.addEventListener(MouseEvent.CLICK, useCoupon_callButtonClickHandler);			yLayoutMultiplier++;						//unsetCoupon ui			xLayoutMultiplier = 0;			if(!unsetCouponLabel){				unsetCouponLabel = new Label();				unsetCouponLabel.label = 'unsetCoupon method';				unsetCouponLabel.displayObject.x = PADDING;				unsetCouponLabel.displayObject.y = PADDING + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(unsetCouponLabel.displayObject);							}												if(!unsetCoupon_callButton){				unsetCoupon_callButton = new Button();				unsetCoupon_callButton.label = "call";				unsetCoupon_callButton.displayObject.x = PADDING + METHOD_INPUT_WIDTH * xLayoutMultiplier;				unsetCoupon_callButton.displayObject.y = PADDING + unsetCouponLabel.displayObject.height  + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(unsetCoupon_callButton.displayObject);			}			unsetCoupon_callButton.addEventListener(MouseEvent.CLICK, unsetCoupon_callButtonClickHandler);			yLayoutMultiplier++;						if(!resultDisplay){				resultDisplay = new TextResultDisplay();				resultDisplay.displayObject.y = 2 * PADDING + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(resultDisplay.displayObject);			}					}	  								private function useCoupon_callButtonClickHandler(event:MouseEvent):void{			_service.useCoupon(useCoupon_couponCode_Input.data).setResultHandler(useCouponResultHandler).setErrorHandler(errorHandler); 		}		private function useCouponResultHandler(obj:Object):void{			resultDisplay.data = "result : \n" + obj + '\n' + ObjectUtil.deepObjectToString(obj) + "\n";		}				private function unsetCoupon_callButtonClickHandler(event:MouseEvent):void{			_service.unsetCoupon().setResultHandler(unsetCouponResultHandler).setErrorHandler(errorHandler); 		}		private function unsetCouponResultHandler(obj:Object):void{			resultDisplay.data = "result : \n" + obj + '\n' + ObjectUtil.deepObjectToString(obj) + "\n";		}						private function errorHandler(obj:Object):void{			resultDisplay.data = "error :  \n" +  ObjectUtil.deepObjectToString(obj) + "\n";		}	}}