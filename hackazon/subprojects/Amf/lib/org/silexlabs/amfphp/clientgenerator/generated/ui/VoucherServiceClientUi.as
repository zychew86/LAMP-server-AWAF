package org.silexlabs.amfphp.clientgenerator.generated.ui{	import fla.Label;	import fla.Button;	import fla.TextInput;	import fla.TextResultDisplay;		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.net.NetConnection;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFieldType;		import org.silexlabs.amfphp.clientgenerator.generated.service.VoucherServiceClient;	import org.silexlabs.amfphp.clientgenerator.NetConnectionSingleton;	import org.silexlabs.amfphp.clientgenerator.ObjectUtil;	import org.silexlabs.amfphp.clientgenerator.ui.elements.IDataIoGui;	import org.silexlabs.amfphp.clientgenerator.ui.elements.ILabel;	import org.silexlabs.amfphp.clientgenerator.generated.service.VoucherServiceClient;
		public class VoucherServiceClientUi extends MovieClip	{		/**		 * layout constants		 * */		private static const PADDING:int = 20;		private static const METHOD_UI_HEIGHT:int = 40;		private static const METHOD_INPUT_WIDTH:int = 120;		private static const CALL_BUTTON_X:int = 400;				private var _service:VoucherServiceClient;						//registerVoucher inputs 		public var registerVoucher_date_Input:IDataIoGui; 		public var registerVoucher_dayOfWeek_Input:IDataIoGui; 		public var registerVoucherLabel:ILabel;		public var registerVoucher_callButton:ILabel;				public var resultDisplay:IDataIoGui;		public function VoucherServiceClientUi()		{			super();			var xLayoutMultiplier:int = 0;			var yLayoutMultiplier:int = 0;			var maxNumMethodInputs:int = 0;						_service = new VoucherServiceClient(NetConnectionSingleton.getNetConnection());			graphics.beginFill(0, 0.3);						//registerVoucher ui			xLayoutMultiplier = 0;			if(!registerVoucherLabel){				registerVoucherLabel = new Label();				registerVoucherLabel.label = 'registerVoucher method';				registerVoucherLabel.displayObject.x = PADDING;				registerVoucherLabel.displayObject.y = PADDING + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(registerVoucherLabel.displayObject);							}									if(!registerVoucher_date_Input){				registerVoucher_date_Input = new TextInput();				registerVoucher_date_Input.displayObject.x = PADDING + METHOD_INPUT_WIDTH * xLayoutMultiplier;				registerVoucher_date_Input.displayObject.y = PADDING + registerVoucherLabel.displayObject.height  + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(registerVoucher_date_Input.displayObject);							}			xLayoutMultiplier++;						if(!registerVoucher_dayOfWeek_Input){				registerVoucher_dayOfWeek_Input = new TextInput();				registerVoucher_dayOfWeek_Input.displayObject.x = PADDING + METHOD_INPUT_WIDTH * xLayoutMultiplier;				registerVoucher_dayOfWeek_Input.displayObject.y = PADDING + registerVoucherLabel.displayObject.height  + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(registerVoucher_dayOfWeek_Input.displayObject);							}			xLayoutMultiplier++;									if(!registerVoucher_callButton){				registerVoucher_callButton = new Button();				registerVoucher_callButton.label = "call";				registerVoucher_callButton.displayObject.x = PADDING + METHOD_INPUT_WIDTH * xLayoutMultiplier;				registerVoucher_callButton.displayObject.y = PADDING + registerVoucherLabel.displayObject.height  + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(registerVoucher_callButton.displayObject);			}			registerVoucher_callButton.addEventListener(MouseEvent.CLICK, registerVoucher_callButtonClickHandler);			yLayoutMultiplier++;						if(!resultDisplay){				resultDisplay = new TextResultDisplay();				resultDisplay.displayObject.y = 2 * PADDING + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(resultDisplay.displayObject);			}					}	  								private function registerVoucher_callButtonClickHandler(event:MouseEvent):void{			_service.registerVoucher(registerVoucher_date_Input.data, registerVoucher_dayOfWeek_Input.data).setResultHandler(registerVoucherResultHandler).setErrorHandler(errorHandler); 		}		private function registerVoucherResultHandler(obj:Object):void{			resultDisplay.data = "result : \n" + obj + '\n' + ObjectUtil.deepObjectToString(obj) + "\n";		}						private function errorHandler(obj:Object):void{			resultDisplay.data = "error :  \n" +  ObjectUtil.deepObjectToString(obj) + "\n";		}	}}